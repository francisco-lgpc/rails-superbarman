<%= simple_form_for @party, url: "/bartenders/search", method: :get do |f| %>
  <%= f.input :address, collection: Party::CITIES, label: false, prompt: 'Location', wrapper_html: { class: '' }, input_html: { class: "input form-input-index" } %>


  <%= f.input :date, label: false, as: :string, input_html: { class: "datepicker" } %>

  <%= button_tag type: :submit, class: "btn-search", id: 'btn-search-index' do %>
    <i class="fa fa-search"></i>
  <% end %>

<% end %>

